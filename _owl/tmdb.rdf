  <owl:Class rdf:about="&tmdb;Creator">
    <rdfs:label xml:lang="en">Creator</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the creators class. This class represents a creator of the list tv/created_by.</rdfs:comment>
  </owl:Class>

