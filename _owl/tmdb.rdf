  <!-- OWL Header -->
  <owl:Ontology rdf:about="http://semmul2014.hpi.de/themoviedb.owl#">
    <dc:title>The Movie Database (TMDb) Ontology</dc:title>
    <dc:description>Ontology for The Movie Database that contains all attributes taken from the TMDb API. There are additional classes to represent lists. Prefixes (separated with two hyphens) make the attributes unique. The last part of the properties' name (after the last two hyphens) matches exactly the name in the JSON format. To map a list (e.g. 'genres'), use the property (e.g. 'movie/genres')</dc:description>
  </owl:Ontology>

  <!-- CLASSES -->

  <!-- Alternative_Title -->
  <owl:Class rdf:about="&tmdb;Alternative_Title">
    <rdfs:label xml:lang="en">Alternative_Title</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the alternative titles class. This class represents an alternative title of the list movie/alternative_titles.</rdfs:comment>
  </owl:Class>

  <!-- Cast -->
  <owl:Class rdf:about="&tmdb;Cast">
    <rdfs:label xml:lang="en">Cast</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the cast class for a movie or tv series. This class represents a cast member of the list movie/credits/cast or tv/credits/cast.</rdfs:comment>
  </owl:Class>

  <!-- Collection -->
  <owl:Class rdf:about="&tmdb;Collection">
    <rdfs:label xml:lang="en">Collection</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the collection a movie belongs to.</rdfs:comment>
  </owl:Class>

  <!-- Combined_Cast -->
  <owl:Class rdf:about="&tmdb;Combined_Cast">
    <rdfs:label xml:lang="en">Combined_Cast</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the combined cast class for a person. This class represents a cast from a person of the list person/combined_credits/cast.</rdfs:comment>
  </owl:Class>

  <!-- Combined_Crew -->
  <owl:Class rdf:about="&tmdb;Combined_Crew">
    <rdfs:label xml:lang="en">Combined_Crew</rdfs:label>
    <rdfs:comment xml:lang="en">Defines the combined crew class for a person. This class represents a crew from a person of the list person/combined_credits/crew.</rdfs:comment>
  </owl:Class>

